<Window x:Class="warring.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:warring"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Start" HorizontalAlignment="Left" Margin="120,242,0,0" VerticalAlignment="Top" Width="120" Height="56" Click="Button_Click" />
        <TextBox x:Name="HZ" HorizontalAlignment="Left" Height="23" Margin="120,131,0,0" TextWrapping="Wrap" Text="800" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Time" HorizontalAlignment="Left" Height="23" Margin="322,131,0,0" TextWrapping="Wrap" Text="200" VerticalAlignment="Top" Width="120"/>
        <Button Content="Stop" HorizontalAlignment="Left" Margin="322,242,0,0" VerticalAlignment="Top" Width="120" Height="56" Click="Button_Click_1" />

    </Grid>
</Window>
